{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "rust: cargo debug build"
		},
		{
			"type": "cargo",
			"command": "build",
			"args": ["--release"],
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo release build"
		}
	]
}